eto/perl6-Algorithm-Soundex.git","perl":"6.*","name":"Algorithm::Soundex","provides":{"Algorithm::Soundex":"lib/Algorithm/Soundex.pm"},"license":"Artistic-2.0"},{"provides":{"MongoDB::Uri":"lib/MongoDB/Uri.pm6","MongoDB::Server":"lib/MongoDB/Server.pm6","MongoDB::Server::Control":"lib/MongoDB/Server/Control.pm6","MongoDB::Header":"lib/MongoDB/Header.pm6","MongoDB::Database":"lib/MongoDB/Database.pm6","MongoDB::MDBConfig":"lib/MongoDB/MDBConfig.pm6","MongoDB::HL::Users":"lib/MongoDB/HL/Users.pm6","MongoDB::Cursor":"lib/MongoDB/Cursor.pm6","MongoDB::Client":"lib/MongoDB/Client.pm6","MongoDB::Wire":"lib/MongoDB/Wire.pm6","MongoDB":"lib/MongoDB.pm6","MongoDB::Server::Monitor":"lib/MongoDB/Server/Monitor.pm6","MongoDB::Server::Socket":"lib/MongoDB/Server/Socket.pm6","MongoDB::Authenticate::Credential":"lib/MongoDB/Authenticate/Credential.pm6","MongoDB::Authenticate::Scram":"lib/MongoDB/Authenticate/Scram.pm6","MongoDB::HL::Collection":"lib/MongoDB/HL/Collection.pm6","MongoDB::Collection":"lib/MongoDB/Collection.pm6","MongoDB::Log":"lib/MongoDB/Log.pm6"},"license":"Artistic-2.0","name":"MongoDB","perl":"6","authors":["Marcel Timmerman","Paweł Pabian"],"version":"0.40.3.3","description":"MongoDB driver","source-url":"git://github.com/MARTIMM/mongo-perl6-driver.git","tags":["NoSQL","database","mongodb","driver"],"test-depends":["Test::META"],"depends":["BSON","Semaphore::ReadersWriters","Config::DataLang::Refine","Auth::SCRAM","Base64","OpenSSL","URI::Escape","Unicode::PRECIS","Terminal::ANSIColor","Log::Async"],"build-depends":[]},{"version":"0.1.0","provides":{"SDL::Event":"lib/lib/SDL/Event.pm6","SDL::Color":"lib/lib/SDL/Color.pm","SDL::Mixer::Channels":"lib/lib/SDL/Mixer/Channels.pm6","SDL::Surface":"lib/lib/SDL/Surface.pm6","SDL::Controller":"lib/lib/SDL/Controller.pm6","SDL::Mixer":"lib/lib/SDL/Mixer.pm6","SDL::PixelFormat":"lib/lib/SDL/PixelFormat.pm6","SDL::Mixer::Effects":"lib/lib/SDL/Mixer/Effects.pm6","SDL::CStructs":"lib/lib/SDL/CStructs.pm6","SDL":"lib/lib/SDL.